{
  "name": "CoMude",
  "version": "2.0",
  "permissions": [ "storage", "tabs", "contextMenus", "bookmarks", "https://*/", "identity", "activeTab" ],
  "manifest_version": 2,
  "minimum_chrome_version": "23",

  "key": "",
  "description": "Let's talk about the mood in here.",
  "oauth2": {
    "client_id": "212628296953-b4ke8a30bu118t3rk7ehl5sg905pvb71.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/gmail.compose",
      "https://www.gmail.com/*"
    ]
  },

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "content.js" ]
    }
  ],

  "content_security_policy": "script-src 'self'; script-src-elem 'self' data: blob:; worker-src 'self' data: blob:; object-src 'self'",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "CoMude! Tell us how you're doing."

  },

  "icons": { "64": "favicon.png" },

  "omnibox": {
    "keyword": "help"
  }

}